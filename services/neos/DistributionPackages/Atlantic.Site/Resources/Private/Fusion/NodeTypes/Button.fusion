prototype(Atlantic.Site:Content.Button) < prototype(Neos.Neos:ContentComponent) {
    content = Neos.Neos:Editable {
        property = 'content'
    }

    @context.type = ${q(node).property('type')}

    renderer = afx`
        <button type={props.type} class='Button'>{props.content}</button>
    `
}
